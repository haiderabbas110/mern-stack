<%- include('include/header'); %>
    <h1>Update User</h1>
    <form action="/items/update/<%=userData._id%>" method="POST">
        <div>
            <label for="">Name</label>
            <input type="name" name="name" value="<%=userData.name%>" placeholder="Enter your name">
        </div>
        <div>
            <label for="">Email</label>
            <input type="email" name="email" value="<%=userData.email%>" placeholder="Enter your email">
        </div>
        <div>
            <label for="">Phone</label>
            <input type="phone" name="phone" value="<%=userData.phone%>" placeholder="Enter your phone">
        </div>
        <div>
            <input type="submit" value="Submit">
        </div>
    </form>

    </body>

    </html>